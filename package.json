{
  "name": "carmen-turborepo-frontend",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "dev:sample": "turbo run dev:sample",
    "build:sample": "turbo run build:sample",
    "start:sample": "turbo run start:sample",
    "lint:sample": "turbo run lint:sample",
    "check-types:sample": "turbo run check-types:sample"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.4.4",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.1.20",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "trustedDependencies": [
    "carmen-inventory",
    "snappy"
  ]
}
